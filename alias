# Generic file viewing aliases
if [ -f /etc/issue ]; then
  alias ls='ls -G --color'
  alias ll='ls -alGh --color'
else
  alias ls='ls -FHG'
  alias ll='ls -alhFHG'
  alias python='/usr/local/opt/python/libexec/bin/python2'
fi
