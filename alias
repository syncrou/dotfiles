# Generic file viewing aliases
if [ -f /etc/issue ]; then
  alias ls='ls -G --color'
  alias ll='ls -alGh --color'
else
  alias ls='ls -FHG'
  alias ll='ls -alhFHG'
fi
# Bi ssh aliases
alias bis='ssh -p 8022'
alias bisc='scp -P 8022'

# git aliases
